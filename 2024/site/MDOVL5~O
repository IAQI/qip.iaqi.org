              $("a[href$='.gif']").attr("class", "group1").attr("rel", "imggroup");
              $("a[href$='.GIF']").attr("class", "group1").attr("rel", "imggroup");
          });

          $(document).ready(function () {
              /* Apply fancybox to multiple items */
